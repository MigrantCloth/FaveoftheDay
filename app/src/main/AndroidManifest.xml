<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.faveoftheday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OutKast"
            android:label="@string/category_OutKast"/>
        <activity android:name=".Rakim"
            android:label="@string/category_Rakim"/>
        <activity android:name=".AnitaBaker"
            android:label="@string/category_Anita_Baker"/>
        <activity android:name=".BoyzIIMen"
            android:label="@string/category_Boyz_II_Men"></activity>
    </application>

</manifest>